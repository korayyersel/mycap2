        }
      }
    },
    "auth": {
      "passport": {
        "strategy": "mock",
        "users": {
          "koray": {
            "roles": [
              "mycap2role"
            ],
            "attributes": {
              "country": "CH"
            }
          },
          "koray2": {
            "roles": [
              "xxx"
            ]
          }
        }
      }
    }
  },
  "devDependencies": {
    "sqlite3": "^4.1.1"
  }
}